    <description>Adds Calendar events to follow up on outstanding Evaluations</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>datevalue([Eval_Request__c].Latest_Ship_Date__c ) = today()  
&amp;&amp;( text([Eval_Request__c].Eval_Type__c ) = &apos;End-User/Reseller/Disty Eval&apos;
|| text([Eval_Request__c].Eval_Type__c ) = &apos;Reseller Demo&apos;)
