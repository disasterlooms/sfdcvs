<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>49.0</apiVersion>
    <constants>
        <name>one</name>
        <dataType>String</dataType>
        <value>
            <stringValue>1</stringValue>
        </value>
    </constants>
    <constants>
        <name>quote</name>
        <dataType>String</dataType>
        <value>
            <stringValue>quote</stringValue>
        </value>
    </constants>
    <description>gets the docid from the quote and automatically adds it to email as an attachment</description>
    <formulas>
        <name>fivemin</name>
        <dataType>DateTime</dataType>
        <expression>({!$Flow.CurrentDateTime})+(5/1440)</expression>
    </formulas>
    <formulas>
        <name>onehour</name>
        <dataType>Number</dataType>
        <expression>1</expression>
        <scale>2</scale>
    </formulas>
    <interviewLabel>EvalQuoteDocId {!$Flow.CurrentDateTime}</interviewLabel>
    <label>EvalQuoteDocId</label>
    <processType>AutoLaunchedFlow</processType>
    <recordLookups>
        <name>Find_Document_Id</name>
        <label>Find Document Id</label>
        <locationX>334</locationX>
        <locationY>56</locationY>
        <assignNullValuesIfNoRecordsFound>false</assignNullValuesIfNoRecordsFound>
        <connector>
            <targetReference>Update_Eval_Page_with_docid</targetReference>
        </connector>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Name</field>
            <operator>Contains</operator>
            <value>
                <elementReference>quote</elementReference>
            </value>
        </filters>
        <filters>
            <field>Name</field>
            <operator>Contains</operator>
            <value>
                <elementReference>evalname</elementReference>
            </value>
        </filters>
        <object>Document</object>
        <outputAssignments>
            <assignToReference>docid</assignToReference>
            <field>Id</field>
        </outputAssignments>
        <sortField>CreatedDate</sortField>
        <sortOrder>Desc</sortOrder>
    </recordLookups>
    <recordUpdates>
        <name>Update_Eval_Page_with_docid</name>
        <label>Update Eval Page with docid</label>
        <locationX>546</locationX>
        <locationY>53</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>Name</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>evalname</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>Eval_Quote_Sent_Out__c</field>
            <value>
                <booleanValue>false</booleanValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>Quotedocid__c</field>
            <value>
                <elementReference>docid</elementReference>
            </value>
        </inputAssignments>
        <object>Eval_Request__c</object>
    </recordUpdates>
    <startElementReference>Wait_five_minutes</startElementReference>
    <status>Active</status>
    <variables>
        <name>docid</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>evalname</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <variables>
        <name>time</name>
        <dataType>String</dataType>
        <isCollection>false</isCollection>
        <isInput>true</isInput>
        <isOutput>true</isOutput>
    </variables>
    <waits>
        <name>Wait_five_minutes</name>
        <label>Wait five minutes</label>
        <locationX>121</locationX>
        <locationY>58</locationY>
        <defaultConnectorLabel>[Default Path]</defaultConnectorLabel>
        <waitEvents>
            <name>X5_minutes</name>
            <conditionLogic>and</conditionLogic>
            <connector>
                <targetReference>Find_Document_Id</targetReference>
            </connector>
            <eventType>AlarmEvent</eventType>
            <inputParameters>
                <name>AlarmTime</name>
                <value>
                    <elementReference>fivemin</elementReference>
                </value>
            </inputParameters>
            <label>5 minutes</label>
            <outputParameters>
                <assignToReference>time</assignToReference>
                <name>AlarmTime</name>
            </outputParameters>
        </waitEvents>
    </waits>
</Flow>
