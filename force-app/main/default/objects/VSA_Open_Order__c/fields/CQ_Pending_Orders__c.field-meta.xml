<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CQ_Pending_Orders__c</fullName>
    <externalId>false</externalId>
    <formula>Status__c == &apos;Pending&apos; &amp;&amp; 
    CASE(MONTH(TODAY()),        
         2,3,
         3,3,
         4,3,
         5,4,
         6,4,
         7,4,
         8,1,
         9,1,
         10,1,
         2) =
      CASE(MONTH(Ship_Date__c),
           2,3,
           3,3,
           4,3,
           5,4,
           6,4,
           7,4,
           8,1,
           9,1,
          10,1,
          2)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>CQ Pending Orders</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
