<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Product_Name_5_is_blank</fullName>
    <active>true</active>
    <errorConditionFormula>AND((not(isblank(Product_Name_5__c))),(OR(ISBLANK(Quantity_5__c),isnull(Quantity_5__c))))</errorConditionFormula>
    <errorDisplayField>Quantity_5__c</errorDisplayField>
    <errorMessage>Input quantity because there is a product</errorMessage>
</ValidationRule>
